#include <iostream>
using namespace std;

int main() {
    int k, n;
    cout << "Enter the number k: ";
    cin >> k;
    cout << "Enter the exponent n (must be a natural number): ";
    cin >> n;

    int result = 1; 
    for (int i = 0; i < n; ++i) {
        result *= k; 
    }

    cout << k << " raised to the power of " << n << " equals " << result << endl;
    return 0;
}
